{
  "cSpell.language": "en-GB",
  "cSpell.words": [
    "Adsetts",
    "anonymised",
    "Anyameluhor",
    "architecting",
    "biblatex",
    "confirmee",
    "confirmer",
    "Connecteam",
    "datetime",
    "geofence",
    "geofences",
    "Hons",
    "HOTP",
    "Hubstaff",
    "kanban",
    "laravel",
    "Legrand",
    "LINQ",
    "microservices",
    "multifactor",
    "Nnamdi",
    "nvarchar",
    "timescale",
    "timescales",
    "timestep",
    "TOTP",
    "uniqueidentifier",
    "userbase"
  ],

  "[markdown]": {
    "editor.rulers": [60]
  },

  "[latex]": {
    "editor.rulers": [60]
  },

  "[bibtex]": {
    "editor.rulers": [60]
  },

  "latex-workshop.latex.autoBuild.run": "never",
  // powershell 7
  "latex-workshop.latex.external.build.command": "pwsh.exe",
  // relative to workspace
  "latex-workshop.latex.external.build.args": ["./docs/report/build.ps1"],

  "latex-workshop.latexindent.args": [
    // "-c=%WORKSPACE_FOLDER%/docs/report/build/",
    "-l=%WORKSPACE_FOLDER%/latexindent.yaml",
    "-m",
    "%TMPFILE%"
  ]
}
