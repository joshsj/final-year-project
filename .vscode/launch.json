{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build App",
      "program": "${workspaceFolder}/src/WebUI/bin/Debug/net6.0/RendezVous.WebUI.dll",
      "cwd": "${workspaceFolder}/src/WebUI",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      }
    }
  ]
}
