{
  "[markdown]": {
    "editor.rulers": [61]
  },

  "cSpell.language": "en-GB",
  "cSpell.words": [
    "Anyameluhor",
    "Hons",
    "Nnamdi",
    "Timescale",
    "Timescales",
    "anonymised"
  ],

  "[latex]": {
    "editor.formatOnSave": false,
    "editor.suggestSelection": "recentlyUsedByPrefix"
  },

  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.latex.external.build.command": "pwsh.exe", // powershell 7
  "latex-workshop.latex.external.build.args": ["./docs/report/build.ps1"] // relative to workspace
}
